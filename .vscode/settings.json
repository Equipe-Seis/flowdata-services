//.vscode\settings.json
{
  "editor.formatOnSave": true,
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "files.associations": {
    "*.prisma": "prisma"
  },
  "typescript.tsdk": "node_modules/typescript/lib", // Isso garante que o VSCode use a vers√£o local do TypeScript
  "typescript.preferences.importModuleSpecifier": "non-relative" // Usar alias do TypeScript para imports
}